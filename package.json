{
  "name": "synticks-chart",
  "version": "0.0.1",
  "description": "A playful real-time candlestick charting engine by Ondra.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.cjs",
      "types": "./dist/vue.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts src/vue.ts --dts --format cjs,esm --sourcemap --clean",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "dev": "vite dev --config demo/vite.config.ts"
  },
  "keywords": [
    "chart",
    "candlestick",
    "ohlcv",
    "trading",
    "vue",
    "typescript",
    "synticks"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ondrahracek/synticks-chart.git"
  },
  "author": "Ondřej Hráček <ondra.hracek@seznam.cz>",
  "bugs": {
    "url": "https://github.com/ondrahracek/synticks-chart/issues"
  },
  "homepage": "https://github.com/ondrahracek/synticks-chart#readme",
  "devDependencies": {
    "@types/node": "^22.x",
    "@vitest/ui": "^1.6.1",
    "eslint": "^9.x",
    "tsup": "^8.x",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  }
}
